{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Resources": {
    "tablestudygurubathrooms": {
      "Type": "AWS::DynamoDB::Table",
      "Properties": {
        "AttributeDefinitions": [
          {
            "AttributeName": "unique_id",
            "AttributeType": "S"
          }
        ],
        "KeySchema": [
          {
            "AttributeName": "unique_id",
            "KeyType": "HASH"
          }
        ],
        "ProvisionedThroughput": {
          "ReadCapacityUnits": "1",
          "WriteCapacityUnits": "1"
        }
      }
    },
    "tablestudygurubathroomsconfig": {
      "Type": "AWS::DynamoDB::Table",
      "Properties": {
        "AttributeDefinitions": [
          {
            "AttributeName": "config_date_hash",
            "AttributeType": "S"
          },
          {
            "AttributeName": "current_config",
            "AttributeType": "Boolean"
          },
          {
            "AttributeName": "config_date",
            "AttributeType": "S"
          }
        ],
        "KeySchema": [
          {
            "AttributeName": "config_date_hash",
            "KeyType": "HASH"
          }
        ],
        "ProvisionedThroughput": {
          "ReadCapacityUnits": "1",
          "WriteCapacityUnits": "1"
        }
      }
    },
    "s3awsbathroomappfiles": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "AccessControl": "Private",
        "CorsConfiguration": {
          "CorsRules": [
            {
              "AllowedHeaders": [
                "Authorization"
              ],
              "AllowedMethods": [
                "GET"
              ],
              "AllowedOrigins": [
                "*"
              ],
              "MaxAge": "3000"
            }
          ]
        },
        "VersioningConfiguration": {
          "Status": "Suspended"
        }
      }
    }
  },
{
  "Type" : "AWS::IAM::Policy",
  "Properties" : { 
    "Groups" : [ String, ... ],
    "PolicyDocument" : JSON object,
    "PolicyName" : String,
    "Roles" : [ String, ... ],
  }
},
{
  "Type": "AWS::IAM::Role",
  "Properties": {
    "AssumeRolePolicyDocument": { JSON },
    "ManagedPolicyArns": [ String, ... ],
    "Path": String,
    "Policies": [ Policies, ... ],
    "RoleName": "bathroom_app_get_status_lambda_role"
  }
},
{
  "Type": "AWS::IAM::Role",
  "Properties": {
    "AssumeRolePolicyDocument": { JSON },
    "ManagedPolicyArns": [ String, ... ],
    "Path": String,
    "Policies": [ Policies, ... ],
    "RoleName": "bathroom_app_set_status_lambda_role"
  }
},
{
  "Type": "AWS::IAM::Role",
  "Properties": {
    "AssumeRolePolicyDocument": { JSON },
    "ManagedPolicyArns": [ String, ... ],
    "Path": String,
    "Policies": [ Policies, ... ],
    "RoleName": "bathroom_app_alexa_lambda_role"
  }
},
{
  "Type": "AWS::IAM::Role",
  "Properties": {
    "AssumeRolePolicyDocument": { JSON },
    "ManagedPolicyArns": [ String, ... ],
    "Path": String,
    "Policies": [ Policies, ... ],
    "RoleName": "bathroom_app_apply_config_lambda_role"
  }
},
{
  "Type": "AWS::IAM::Role",
  "Properties": {
    "AssumeRolePolicyDocument": { JSON },
    "ManagedPolicyArns": [ String, ... ],
    "Path": String,
    "Policies": [ Policies, ... ],
    "RoleName": "bathroom_app_update_config_lambda_role"
  }
}


  "Description": "The Bathroom App"
}